# PROBABILITY DEFINITION
import random
coin=['H','T']
n=100
c=0
r=0
for i in range(1,n+1):
    flip=random.choice(coin)
    if flip=='H':
        c=c+1
        print(i,flip,c,c/i)
        
    else:
        
        print(i,flip,c,c/i)
          